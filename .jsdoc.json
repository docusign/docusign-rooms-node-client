{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src", "package.json"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false
  },
  "opts": {
    "source": "./src/",
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/docdash"
  },
  "docdash": {
    "static": false,
    "sort": true
  }
}